{
  "name": "acg",
  "private": true,
  "type": "module",
  "version": "0.5.26",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "sync": "tsx scripts/sync.ts",
    "sync:dry": "tsx scripts/sync.ts --dry-run",
    "validate": "tsx scripts/validate-generated-data.ts",
    "summary": "tsx scripts/step-summary.ts",
    "lint": "eslint .",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "lhci": "npx --yes @lhci/cli@0.15.1 autorun --config .lighthouserc.json",
    "lhci:simulate": "npx --yes @lhci/cli@0.15.1 autorun --config .lighthouserc.simulate.json",
    "lhci:local": "tsx scripts/lhci-local.ts",
    "lhci:local:simulate": "tsx scripts/lhci-local.ts --simulate",
    "lhci:summary": "tsx scripts/lhci-step-summary.ts",
    "budget": "tsx scripts/perf-budget.ts",
    "check": "astro check",
    "test": "node --import tsx --test tests/index.test.ts",
    "test:coverage": "c8 --reporter=text-summary --reporter=lcov node --import tsx --test tests/index.test.ts"
  },
  "dependencies": {
    "@astrojs/tailwind": "^6.0.2",
    "astro": "^5.16.5",
    "cheerio": "^1.1.2",
    "fast-xml-parser": "^5.3.3"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.6",
    "@eslint/js": "^9.39.2",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "autoprefixer": "^10.4.23",
    "c8": "^10.1.3",
    "eslint": "^9.39.2",
    "eslint-plugin-astro": "^1.5.0",
    "globals": "^17.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "tailwindcss": "^3.4.19",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
